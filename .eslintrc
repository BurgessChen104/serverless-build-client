{
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "no-undef": "error",
    "no-unused-vars": "error",
    "quotes": ["error", "double"],
    "prettier/prettier": "error"
  }
}
